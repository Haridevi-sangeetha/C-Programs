#include<stdio.h>
#include<limits.h>

int main(){
    int size;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    int first_max=arr[0];
    for(int i=0;i<size;i++){
        if(arr[i]>first_max){
            first_max=arr[i];
        }
    }
    int second_max = INT_MIN;
    for(int i=0;i<size;i++){
        if(arr[i]>=second_max && arr[i]<first_max){
            second_max=arr[i];
        }
    }
    printf("Largest : %d\n",first_max);
    printf("Second Lorgest : %d",second_max);
    return 0;
}
